<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.atozfit">

    <application
        android:allowBackup="true"
        android:icon="@drawable/bp_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AtoZFit">
        <activity android:name=".Activity.GraphViewer"></activity>
        <activity android:name=".Activity.Registration" />
        <activity android:name=".Activity.DisplayBP_Results"/>
        <activity android:name=".Activity.AddBPReadings"/>
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>